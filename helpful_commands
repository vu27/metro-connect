//                    DJANGO                  //

// Run server
python manage.py runserver

// Create super user
python manage.py createsuperuser

// Create staticfiles
python manage.py collectstatic

//                   DATABASE                 //

// Add Database migration
python manage.py makemigrations [app-name]

// Update Database
python3 manage.py migrate

// RESET DATABASE
rm [app-name]/migrations/*
rm db.sqlite3
python manage.py makemigrations
python manage.py migrate


//                 ENVIRONMENT                //

// Start environment
source venv/Scripts/activate

// Install requirements.txt
sudo pip install -r requirements.txt

// SHOW all dependencies installed on current virtual environment 
pip freeze

// GO TO ROOT ON MAC
CMD SHFT G

// Make sure python33 is install
// Make sure pipenv is installed pip3 install pipenv

// Create new virtual environment
pipenv shell

// Install pipenv defaults (versions matter)
pipenv install django==3.0.5 djangorestframework==3.11.0 django-rest-knox==4.1.0 

// Find process id 
sudo lsof -i :35729

// Kill process
sudo kill -9 30727


//                    GIT                  //

// MERGE CONFLICT HELPER
git mergetool

// Revert back to old commit
git revert --no-commit 0766c053..HEAD
git commit

// See which branch you are on 
git branch

// See status of git
git status

// See all git logs (only shows you logs of CURRENT BRANCH you're on)
git log

// Stage new edits
git add [file name OR . for all]

// Commit with message
git commit -m "this is the message"

// Push changes onto master branch
git push

// Switching to separate branches or old commits
git checkout [branch name]

// Return to master branch
git checkout master

// Make new branch so you can make edits that don't affect the MASTER branch
git branch [name of branch]
(MAKE SURE YOU "git checkout [name of branch" to go to that branch to make edits)

// MERGE updates from a separate branch to the MASTER branch
git merge [name of branch]

// if you recieve fatal: in unpopulated submodule 
git rm --cached . -rf


